<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <!-- ROUNABOUT SIGNS -->
  <node
      name  = "GIR_Q"
      pkg   = "gazebo_ros"
      exec = "spawn_entity.py"
      args  = "-file $(find-pkg-share models_pkg)/roundabout_sign/model.sdf -entity GIR_Q -x 8.766 -y -4.179 -z 0.078998 -R 0  -P 0  -Y 0"/>

  <node
      name  = "GIR_R"
      pkg   = "gazebo_ros"
      exec = "spawn_entity.py"
      args  = "-file $(find-pkg-share models_pkg)/roundabout_sign/model.sdf -entity GIR_R -x 10.320 -y -4.869 -z 0.078998 -R 0  -P 0  -Y 1.570796"/>
  
  <node
      name  = "GIR_S"
      pkg   = "gazebo_ros"
      exec = "spawn_entity.py"
      args  = "-file $(find-pkg-share models_pkg)/roundabout_sign/model.sdf -entity GIR_S -x 11.015 -y -3.297 -z 0.078998 -R 0  -P 0  -Y 3.141592"/>
  
</launch>
